{
  "name": "Avisdepassage",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "podinstallm1": "arch -x86_64 pod install",
    "ios": "react-native run-ios --simulator='iPhone 15 Pro'",
    "ios15": "react-native run-ios --simulator='iPhone 15'",
    "ios_release": "react-native run-ios --configuration Release --udid 00008030-000119A62223802E",
    "ios11": "npx react-native run-ios --udid 00008030-000119A62223802E",
    "iosrv": "npx react-native run-ios --udid 00008110-00083428110A401E",
    "iosj": "npx react-native run-ios --udid FDA08BB4-995C-4224-BC1C-DF4AB372F7FA",
    "iosDevice": "npx react-native run-ios --udid d96652db387ed915ee47c98f2e467190c0c9c731",
    "iosx": "npx react-native run-ios --udid 7769scrollTo2dd441243c1f9ef4aa3668b7a81f09d1ffc2",
    "iosh": "npx react-native run-ios --udid 9E66DE8B-3A56-454B-A1E4-AEC4287E3353",
    "start": "npx react-native start",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "test": "jest",
    "lint": "eslint .",
    "remove-drawable": "cd android && cd app && cd components && cd main && cd res && rm -rf drawable-hdpi && rm -rf drawable-mdpi && rm -rf drawable-xhdpi && rm -rf drawable-xxhdpi && rm -rf drawable-xxxhdpi",
    "remove-raw": "cd android && cd app && cd components && cd main && cd res && rm -rf raw",
    "androidbundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "generate-apk": "cd android && ./gradlew assembleDebug && start .\\app\\build\\outputs\\apk\\",
    "generate-release-apk": "npm run remove-drawable && npm run remove-raw && cd android && ./gradlew assembleRelease && start .\\app\\build\\outputs\\apk\\",
    "generate-build": "npm run remove-drawable && npm run remove-raw && cd android && ./gradlew bundleRelease && start .\\app\\build\\outputs\\apk\\",
    "server-restart": "react-native start --reset-cache",
    "clean": "watchman watch-del-all && rm -rf node_modules && rm -rf yarn.lock && yarn cache clean && yarn install",
    "devMenu": "adb shell input keyevent 82",
    "androidClean": "cd android && ./gradlew clean && cd ../",
    "iOSClean": "cd ios && rm -rf Pods/ && rm Podfile.lock && pod install && cd ..",
    "cleanAll": "npm run clean && npm run androidClean && npm run iOSClean"
  },
  "dependencies": {
    "@react-native-community/blur": "^4.4.0",
    "@react-native-firebase/analytics": "18.7.3",
    "@react-native-firebase/app": "18.7.3",
    "@react-native-firebase/auth": "18.7.3",
    "@react-native-firebase/database": "18.7.3",
    "@react-native-firebase/firestore": "18.7.3",
    "@react-native-firebase/functions": "18.7.3",
    "@react-native-firebase/storage": "18.7.3",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "moment": "^2.30.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-date-picker": "^4.3.5",
    "react-native-device-info": "^10.12.0",
    "react-native-geocoding": "^0.5.0",
    "react-native-get-random-values": "^1.10.0",
    "react-native-image-picker": "^7.1.0",
    "react-native-maps": "^1.10.1",
    "react-native-mmkv": "^2.11.0",
    "react-native-modal": "^13.0.1",
    "react-native-ratings": "^8.1.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-vector-icons": "^10.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.19",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.3",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
